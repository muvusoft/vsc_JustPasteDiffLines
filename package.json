{
  "name": "just-paste-diff-lines",
  "displayName": "Just Paste Diff Lines",
  "description": "No Complex Rules! - Just Paste + / - diffs and preview/apply them in VSCode",
  "version": "3.0.1",
  "publisher": "muvusoft",
  "engines": {
    "vscode": "^1.99.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onView:diffView",
    "onCommand:justPasteDiff.openPanel"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "diffContainer",
          "title": "Diff Tool",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "diffContainer": [
        {
          "id": "diffView",
          "name": "Diff Panel",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "justPasteDiff.openPanel",
        "title": "Just Paste Diff: Open Panel"
      },
      {
        "command": "diffView.applyPatch",
        "title": "Just Paste Diff: Apply Patch"
      },
      {
        "command": "diffView.previewPatch",
        "title": "Just Paste Diff: Preview Patch"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miropharm/vsc_JustPasteDiffLines"
  },
  "bugs": {
    "url": "https://github.com/miropharm/vsc_JustPasteDiffLines/issues"
  },
  "homepage": "https://github.com/miropharm/vsc_JustPasteDiffLines",
  "scripts": {},
  "icon": "media/icon.png"
}